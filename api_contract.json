{
    "title": "Sample API",
    "version": "1.0",
    "endpoints": [
      {
        "path": "/users",
        "methods": [
          {
            "method": "GET",
            "description": "Get a list of users",
            "responses": [
              {
                "code": 200,
                "description": "Success",
                "body": {
                  "type": "application/json",
                  "schema": "User[]"
                }
              }
            ]
          },
          {
            "method": "POST",
            "description": "Create a new user",
            "parameters": [
              {
                "name": "user",
                "in": "body",
                "type": "object",
                "schema": "User"
              }
            ],
            "responses": [
              {
                "code": 201,
                "description": "User created successfully"
              }
            ]
          }
        ]
      }
    ],
    "definitions": {
      "User": {
        "type": "object",
        "properties": {
          "id": { "type": "integer" },
          "name": { "type": "string" },
          "email": { "type": "string", "format": "email" }
          "phoneNumber": { "type": "string", "format": "number" }
        }
      }
    }
  }  
